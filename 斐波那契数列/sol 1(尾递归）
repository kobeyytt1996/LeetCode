class Solution {
public:
int Fibonacci01(int n, int ac1 = 0, int ac2 = 1) {
    if(n == 1) return ac2;
    if (n == 0) return 0;
    int temp = ac1 % 1000000007;
    ac1 = ac2 % 1000000007;
    ac2 = (temp + ac1) % 1000000007;
    return Fibonacci01(n - 1, ac1, ac2);
}
int fib(int n){

   return Fibonacci01(n);
    
}



};
