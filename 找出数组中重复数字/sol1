int findRepeatNumber(vector<int>& nums) {
	if (nums.size() <= 0)
		return -1;

	for (int i = 0; i != nums.size(); i++)
		if (nums[i] < 0 || nums[i] > nums.size() - 1)
			return -1;

	for (int i = 0; i != nums.size(); i++)
		if (nums[i] != i) {
			if (nums[i] != nums[nums[i]]) {
				int temp = nums[i];
				nums[i] = nums[nums[i]];
				nums[nums[i]] = temp;
			}
			else return nums[i];
			i--;
		}
	return -1;

}
